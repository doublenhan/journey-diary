{
    "name": "functions",
    "description": "Cloud Functions for Firebase - Cloudinary image deletion",
    "main": "lib/index.js",
    "type": "commonjs",
    "engines": {
        "node": "20"
    },
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "dependencies": {
        "@types/cors": "^2.8.19",
        "cloudinary": "^2.7.0",
        "cors": "^2.8.5",
        "firebase-admin": "^13.6.0",
        "firebase-functions": "^6.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    }
}
